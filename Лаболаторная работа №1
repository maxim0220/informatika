#include <iostream>
#include <math.h>
void main()
{
    setlocale(LC_ALL, "Russian");
    float a=1000,b=0.0001,c,d,n,h,f,s;
    // разделим пример на части 
    c = pow(a + b, 3);
        d = pow(a, 3);
        n =3 * a * pow(b, 2);
        h = pow(b, 3);
    s=(3 * pow(a, 2) * b);
    //высчитаем результат
    f = (c - (d - n)) / (h - s);
    std::cout << "значение выражения =";
    std::cout << f;
}
